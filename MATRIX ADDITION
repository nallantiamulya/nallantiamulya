#include<stdio.h>
void main()
{
    int m1[10][10],m2[10][10],m3[10][10];
    int r,c,i,j;
    printf("enter row and column size\n");
    scanf("%d%d",&r,&c);
    printf("enter m1%dx%d elements\n",r,c);
    for(i=0;i<r;i++)
    {
        for(j=0;j<c;j++)
        {
            scanf("%d",&m1[i][j]);
            printf("enter m2%dx%d elements\n",r,c);
            for(i=0;i<r;i++)
            {
             for(i==0;j<c;j++)
             {
              scanf("%d",&m2[i][j]);
              printf("Addition of two matrix\n");
              for(i=0;i<r;i++)
              {
                  for(j=0;j<=c;j++)
                  {
                  m3[i][j]=m1[i][j]+m2[i][j];
                  printf("%d)",m3[i][j]);
              }
         printf("\n");
              }
             }
