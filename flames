V#include<stdio.h>
#include<string.h>
void main()
char boyname[20];
char girlname[20];
printf("enter boyname:");
get(bname);
printf("enter cute girlname:");
gets(gname);
int a=strlen(boyname);
int b=strlen(girlname);
{
    for(int i=0;i<a;i++)
    {
        for(int j=0;j<b;j++)
        {
            if(boyname[i]==girlname[j])
            {
                boyname[i]='2';
                girlname[j]='2';
            }
        }
    }
    int a1=0;
    for(int i=0;i<a;i++)
    {
        if(boyname[i]!='2')
        a1=a1+1;
    }
    int b1=0;
    for(int i=0;i<b;i++)
    {
        if(girlname[i]!='2')
        b1=b1+1;
    }
    int total=a1+b1;
    switch(total)
    {
        case 1:printf("frient");
               break;
        case 2:printf("love");
               break;
         case 3:printf("affection");
               break;   
        case 4:printf("marriage");
               break;
        case 5:printf("enemy");
               break;
        case 6:printf("sister");
               break;
    default:printf("enter smaller name");
    }
}
